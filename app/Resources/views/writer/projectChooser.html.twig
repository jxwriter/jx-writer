{% extends 'base.html.twig' %}

{% block title %}Choix du projet
{% endblock %}


{% block body %}
    <h1>Choix du projet : </h1>
	
	<ul>
		<li><a href="{{ path("projectForm") }}">Ajouter un projet</a></li>
	</ul>
	
	<h2>Les projets</h2>

    <ul>
    	
	{% for project in projects %}
        <li><a href="{{ path("chooseProject", { "projectId" : project.id }) }}">{{ project.title }}</a></li>
    {% endfor %}

    </ul>
	
	 

 
{% endblock %}
